{
  "name": "exam-note-cards",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run client:build && nest build",
    "tmc:deploy": "rsync -az --delete --exclude node_modules /mypart/tmc/projects/exam-note-cards/ 192.168.0.198:~/exam-note-cards/",
    "client:build": "npm run build --prefix client",
    "client:dev": "npm run dev --prefix client",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run client:build && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/serve-static": "^5.0.4",
    "@types/node": "^24.9.1",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@types/uuid": "^10.0.0"
  }
}
